{
  "name": "ubelix-shelldap",
  "version": "0.2.3",
  "author": "Rolli, Michael <michael.rolli@id.unibe.ch>",
  "summary": "The UBELIX shelldap module installs shelldap from rpm and will provide the possibility to manage .shelldap.rc files.",
  "license": "Apache-2.0",
  "source": "https://github.com/hpc-unibe-ch/puppet-module-shelldap",
  "issues_url": "https://github.com/hpc-unibe-ch/puppet-module-shelldap/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 <6.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
